@model About

<h1>@Model.Title</h1>
<div class="alert alert-primary">@Model.Description</div>

<ul>
@if (Model.Tags is not null)
{
    foreach (var tag in Model.Tags)
    {
        <li>@tag</li>
    }
}
</ul>

<div class="container text-center">
    <div class="row">
        <div class="col border">
            Column
        </div>
        <div class="col border">
            Column
        </div>
        <div class="col border">
            Column
        </div>
    </div>
</div>
